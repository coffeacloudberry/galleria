{
    "headers": [
        {
            "source": "/(.*)",
            "headers": [
                {
                    "key": "X-Content-Type-Options",
                    "value": "nosniff"
                },
                {
                    "key": "X-Frame-Options",
                    "value": "DENY"
                },
                {
                    "key": "X-Robots-Tag",
                    "value": "none"
                }
            ]
        },
        {
            "source": "/",
            "headers": [
                {
                    "key": "Content-Security-Policy",
                    "value": "default-src 'none'; script-src 'self' 'unsafe-eval' 'unsafe-inline' https://api.mapbox.com https://cdnjs.cloudflare.com https://unpkg.com; connect-src 'self' https://api.mapbox.com; img-src 'self' data: blob:; style-src 'self' 'unsafe-inline' https://api.mapbox.com; font-src 'self' data:; worker-src blob:; child-src blob:; frame-ancestors 'none'; base-uri 'none'; form-action 'none'"
                },
                {
                    "key": "Permissions-Policy",
                    "value": "geolocation=(), camera=(), microphone=()"
                },
                {
                    "key": "Referrer-Policy",
                    "value": "strict-origin"
                },
                {
                    "key": "Access-Control-Allow-Origin",
                    "value": "https://www.explorewilder.com"
                }
            ]
        },
        {
            "source": "/(.*).webtrack",
            "headers": [
                {
                    "key": "Content-Type",
                    "value": "font/otf"
                }
            ]
        }
    ],
    "redirects": [
        {
            "source": "/(.*)",
            "has": [
                {
                    "type": "query",
                    "key": "mtm_cid"
                },
                {
                    "type": "query",
                    "key": "pk_cid"
                },
                {
                    "type": "query",
                    "key": "ss_email_id"
                },
                {
                    "type": "query",
                    "key": "vgo_ee"
                },
                {
                    "type": "query",
                    "key": "fbclid"
                },
                {
                    "type": "query",
                    "key": "gclid"
                },
                {
                    "type": "query",
                    "key": "msclkid"
                },
                {
                    "type": "query",
                    "key": "twclid"
                },
                {
                    "type": "query",
                    "key": "yclid"
                },
                {
                    "type": "query",
                    "key": "ymclid"
                },
                {
                    "type": "query",
                    "key": "ysclid"
                },
                {
                    "type": "query",
                    "key": "utm_source"
                },
                {
                    "type": "query",
                    "key": "utm_medium"
                },
                {
                    "type": "query",
                    "key": "utm_campaign"
                },
                {
                    "type": "query",
                    "key": "utm_content"
                },
                {
                    "type": "query",
                    "key": "utm_term"
                }
            ],
            "destination": "/",
            "permanent": true
        }
    ]
}
